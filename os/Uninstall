term.clear()
term.setCursorPos(1,1)
print("We are sorry to see you go!")
print("---------------------------")
textutils.slowPrint("Uninstalling OS...")
shell.run("delete","/os")
shell.run("delete","menu")
shell.run("delete","startup")
sleep(1)
print("___________________________")
textutils.slowPrint("Rebooting OS...")
os.reboot()
